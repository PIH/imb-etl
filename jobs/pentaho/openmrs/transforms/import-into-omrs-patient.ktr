    <connection>OpenMRS</connection>
    <sql>/*
  Extracts basic attributes of a patient that are directly accessible from the patient and person tables
  Currently we are excluding birthtime since it is not collected directly, and not available on all system data models yet
 */
select      person.person_id as patient_id,
			person.uuid as patient_uuid,
            person.gender,
            person.birthdate,
            person.birthdate_estimated,
            person.dead,
            person.death_date,
			if (person.birthdate is null or person.death_date is null, null, TIMESTAMPDIFF(YEAR, person.birthdate, person.death_date)) as age_years_at_death,
			if (person.birthdate is null or person.death_date is null, null, TIMESTAMPDIFF(MONTH, person.birthdate, person.death_date)) as age_months_at_death,
            person.cause_of_death as cause_of_death_id,
			patient.date_created,
            mothers_name,
            fathers_name,
            phone_number,
            health_center_id,
            identifier,
            country,
            state_province,
            county_district,
            city_village,
            postal_code,
            address1,
            address2,
            address3,
            address4,
            address5,
            address6,
			latitude,
			longitude,
			greatest(COALESCE(person.date_created,0), COALESCE(person.date_changed,0), 
                COALESCE(address.date_created,0), COALESCE(address.date_changed,0), 
                COALESCE(health_center.date_created,0), COALESCE(health_center.date_changed,0), 
                COALESCE(mothers_name.date_created,0),COALESCE(mothers_name.date_changed,0),
                COALESCE(fathers_name.date_created,0),COALESCE(fathers_name.date_changed,0),
                COALESCE(phone_number.date_created,0),COALESCE(phone_number.date_changed,0)
            )	as date_updated
            
from        patient 
inner join  person  on patient.patient_id = person.person_id

LEFT JOIN (
	select      p.uuid as patient_uuid,
				i.identifier,
                i.date_created,
                i.date_changed
	from        person p
	left join   patient_identifier i on p.person_id = i.patient_id
	where       i.patient_identifier_id = (
				  select    i.patient_identifier_id
				  from      patient_identifier i
				  where     i.voided = 0
				  and       i.patient_id = p.person_id
				  order by  i.preferred desc, i.date_created desc limit 1
				)
	order by 	p.uuid
) identifier on identifier.patient_uuid=person.uuid

LEFT JOIN (
	select      p.uuid as patient_uuid,
				a.country,
				a.postal_code,
				a.state_province,
				a.county_district,
				a.city_village,
				a.address1,
				a.address2,
				a.address3,
				a.address4,
				a.address5,
				a.address6,
				a.latitude,
				a.longitude,
                a.date_created,
                a.date_changed
	from        person p
	left join   person_address a on p.person_id = a.person_id
	where       a.person_address_id = (
				  select        a.person_address_id
				  from          person_address a
				  where         a.voided = 0
				  and           a.person_id = p.person_id
				  order by      a.preferred desc, a.date_created desc limit 1
				)
	order by 	p.uuid
) address on address.patient_uuid=person.uuid

LEFT JOIN(
	select      p.person_id,
				a.value as health_center_id,
                a.date_created,
                a.date_changed
	from        person p
	left join   person_attribute a on p.person_id = a.person_id
	left join   person_attribute_type t on a.person_attribute_type_id = t.person_attribute_type_id
	where       a.person_attribute_id = (
				  select    a.person_attribute_id
				  from      person_attribute a
				  where     a.voided = 0
				  and       a.person_id = p.person_id
				  AND       a.person_attribute_type_id = t.person_attribute_type_id
				  order by  a.date_created desc limit 1
				)
                and t.name="Health Center"
) health_center on health_center.person_id=patient.patient_id

LEFT JOIN(
	select      p.person_id,
				a.value as mothers_name,
                a.date_created,
                a.date_changed
	from        person p
	left join   person_attribute a on p.person_id = a.person_id
	left join   person_attribute_type t on a.person_attribute_type_id = t.person_attribute_type_id
	where       a.person_attribute_id = (
				  select    a.person_attribute_id
				  from      person_attribute a
				  where     a.voided = 0
				  and       a.person_id = p.person_id
				  AND       a.person_attribute_type_id = t.person_attribute_type_id
				  order by  a.date_created desc limit 1
				)
                and t.name="Mother's Name"
) mothers_name on mothers_name.person_id=patient.patient_id

LEFT JOIN(
	select      p.person_id,
				a.value as fathers_name,
                a.date_created,
                a.date_changed
	from        person p
	left join   person_attribute a on p.person_id = a.person_id
	left join   person_attribute_type t on a.person_attribute_type_id = t.person_attribute_type_id
	where       a.person_attribute_id = (
				  select    a.person_attribute_id
				  from      person_attribute a
				  where     a.voided = 0
				  and       a.person_id = p.person_id
				  AND       a.person_attribute_type_id = t.person_attribute_type_id
				  order by  a.date_created desc limit 1
				)
                and t.name="Father's name"
) fathers_name on fathers_name.person_id=patient.patient_id


LEFT JOIN(
	select      p.person_id,
				a.value as phone_number,
                a.date_created,
                a.date_changed
	from        person p
	left join   person_attribute a on p.person_id = a.person_id
	left join   person_attribute_type t on a.person_attribute_type_id = t.person_attribute_type_id
	where       a.person_attribute_id = (
				  select    a.person_attribute_id
				  from      person_attribute a
				  where     a.voided = 0
				  and       a.person_id = p.person_id
				  AND       a.person_attribute_type_id = t.person_attribute_type_id
				  order by  a.date_created desc limit 1
				)
                and t.name="Phone number"
) phone_number on phone_number.person_id=patient.patient_id


where       person.voided = 0
and         patient.voided = 0
order by	person.uuid</sql>
