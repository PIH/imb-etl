    <connection>OpenMRS</connection>
    <sql>/*
  Extracts basic attributes of a patient that are directly accessible from the patient and person tables
  Currently we are excluding birthtime since it is not collected directly, and not available on all system data models yet
 */
select      n.person_id as patient_id,
			n.uuid as patient_uuid,
            n.gender,
            n.birthdate,
            n.birthdate_estimated,
            n.dead,
            n.death_date,
			if (n.birthdate is null or n.death_date is null, null, TIMESTAMPDIFF(YEAR, n.birthdate, n.death_date)) as age_years_at_death,
			if (n.birthdate is null or n.death_date is null, null, TIMESTAMPDIFF(MONTH, n.birthdate, n.death_date)) as age_months_at_death,
            n.cause_of_death as cause_of_death_id,
			p.date_created
from        patient p
inner join  person n on p.patient_id = n.person_id
where       p.voided = 0
and         n.voided = 0
order by	n.uuid</sql>
    <limit>0</limit>
    <lookup />
